<%= content_tag(:section) do %>
  <%= content_tag(:h2) do %>
    <%= link_to(article.id, article) %>
  <% end %>
  <%= content_tag(:p) do %>
    <%= article.question_body %>
  <% end %>
  <%= content_tag(:p) do %>
    <%= article.tag_list %>
  <% end %>
  <%= content_tag(:p) do %>
    <%= article.created_at.strftime("%Y-%m-%d %H:%M") %>
  <% end %>
  <%= " " %>
  <%= link_to "編集", [:edit, article] %>
  <%= " " %>
  <%= link_to "削除", article, :data => {:confirm => "本当に？"}, :method => :delete %>
<% end %>
